<head>
  <meta charset="utf-8" />
  <style>
  {{css}}
  </style>
</head>
<body>
  <section>
    <nav>
      <ul>
        <li>The pages of {{ projectName }} </li>
      </ul>
    </nav>
    <article>
      <dl>
      {% for page in  pages %}
        <dt>
          <a href="{{ page.build if isBuild else page.serve }}">{{ page.title if page.title else "no-title" }}</a>
        </dt>
        <dd>{{ page.desc }}</dd>
      </div>
      {% endfor %}
    </article>
    <footer>
      <nav>
        <ul>
          <li>
            <small>generated by <a href="https://npm.im/{{ pkg.name }}">{{ pkg.name }}@{{ pkg.version }}</a></small>
          </li>
        </ul>
      </nav>
    </footer>
  </section>
</body>
